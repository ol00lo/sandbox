{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run mouse_sensor_test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/sensor_saver/test/mouse_sensor_test",
            "args": [],
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "LD_LIBRARY_PATH",
                    "value": "${env:LD_LIBRARY_PATH}:/opt/ros/kilted/lib:${workspaceFolder}/install/sensor_saver/lib"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "/usr/bin/gdb",
            "stopAtEntry": false,
            "preLaunchTask": "echo"
        }
    ]
}
